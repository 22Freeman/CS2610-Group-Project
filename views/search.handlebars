      <!-- side -->
      <div class="side">
        <h3> Saved Searches </h3>
          <div class = "itemlist">
{{#if user.tags}}
  {{#each user.tags}}
  <div class="search-container">
  <form action="/search" method="post">
    <input type="hidden" name="tag" value="{{this}}">
    <button class="btn-ss" text="button" type="submit">#{{this}}</button>
  </form>
  <form action="/search/tag/remove" method="post">
    <input type="hidden" name="tag" value="{{this}}">
    <button class="btn-ss-x" text="button" type="submit">X</button>
  </form>
</div>
