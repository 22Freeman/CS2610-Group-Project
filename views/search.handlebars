        <div class="button-container">
        <form id="search" method="post" action="/search">
          <input type="text" placeholder="" class="ns-textinput" name="tag" size="21" maxlength="120" value = "{{tagName}}">
          <input type="submit" value="Search" class="ns-btn ns-btn-search">
        </form>
        <form id="search" method="post" action="/search/tag/add">
          <input type="hidden" name="tag" value = "{{tagName}}">
          <input type="submit" value="Save Search" class="ns-btn ns-btn-savesearch">
        </form>
      </div>


